  "peerDependencies": {
    "@radix-ui/react-slider": "^0.1.4",
    "framer-motion": "^4.x",
    "react": "^17.x",
    "react-dom": "^17.x",
    "react-hotkeys-hook": "^3.x",
    "react-is": "^17.x",
    "react-tooltip": "^4.x",
    "styled-components": "^5.3.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "build": "npm -v && yarn -v && node -v && yarn ttsc -v && yarn tsc -v && yarn lint:prod && ts-node build-utils/build.ts && yarn build:dts",
    "postbuild": "cp -r ./src/styles/*.css ./dist && yarn build:prep:publish",
    "prebuild:dts": "echo 'building dts'",
    "build:dts": "ttsc --emitDeclarationOnly --declarationDir dist --declaration",
    "postbuild:dts": "echo 'done build dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "build:prep:publish": "ts-node build-utils/setupPackage.ts",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "dev": "concurrently 'yarn start' 'yarn proxy' 'yarn open'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:prod": "eslint src --ext .ts,.tsx -c ./.eslintrc.prod.cjs",
    "open": "open -a 'Google Chrome' https://amino-dev.zonos.com:6007",
    "proxy": "local-ssl-proxy --source 6007 --target 6006",
    "svgs:optimize": "svgo -f ./svgReact/smaller-flags -o ./svgReact/svgs",
    "svgs:download:flags": "node ./svgReact/downloadFlags.js",
    "svgs:copy:flags": "node ./svgReact/copySmallerFlags.js",
    "svgs:create": "yarn svgs:react && yarn svgs:index && yarn svgs:format",
    "svgs:format": "eslint svgReact --ext .tsx --fix",
    "svgs:index": "node ./svgReact/createIndexFile.js ./svgReact/flags",
    "svgs:react": "node ./svgReact ./svgReact/svgs ./svgReact/flags",
    "start": "yarn build-storybook && yarn storybook",
    "storybook": "start-storybook -p 6006",
    "version:patch": "npm version patch -m 'Bump version %s' && yarn push:tags",
    "version:minor": "npm version minor -m 'Bump version %s' && yarn push:tags",
    "version:major": "npm version major -m 'Bump version %s' && yarn push:tags",
    "push:tags": "git push --follow-tags"
  }
}
