{
  "name": "@amino-ui/core",
  "version": "3.0.39",
  "description": "Core UI components for Amino",
  "engines": {
    "node": ">= 12"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/index.ts",
    "src/icons/index.ts",
    "src/i18n/index.ts",
    "src/components/Select/index.ts",
    "src/setupPackage.ts"
  ],
  "targets": {
    "index": {
      "distDir": "./dist",
      "source": "src/index.ts",
      "includeNodeModules": [
        "@radix-ui/react-checkbox",
        "@radix-ui/react-radio-group",
        "@radix-ui/react-slider",
        "react-dropzone"
      ]
    },
    "icons": {
      "source": "src/icons/index.ts"
    },
    "i18n": {
      "source": "src/i18n/index.ts",
      "includeNodeModules": [
        "uuid"
      ]
    },
    "select": {
      "source": "src/components/Select/index.ts",
      "includeNodeModules": [
        "react-select"
      ]
    },
    "setupPackage": {
      "distDir": "./dist",
      "source": "src/setupPackage.ts"
    }
  },
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "repository": "git@github.com:amino-ui/core.git",
  "author": "Joshua Beitler <joshbeitler@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@parcel/core": "^2.5.0",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addons": "6.4.19",
    "@storybook/react": "6.4.19",
    "@storybook/theming": "6.4.19",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.13",
    "@types/styled-components": "5.1.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autoprefixer": "10.4.4",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "downshift": "^4.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-parcel2": "^1.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "framer-motion": "^4.1.17",
    "parcel": "^2.5.0",
    "postcss": "8.4.12",
    "postcss-preset-env": "6.7.1",
    "prettier": "2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "3.4.4",
    "react-is": "^17.0.2",
    "react-tooltip": "^4.2.21",
    "storybook-addon-designs": "6.2.1",
    "styled-components": "5.3.3",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^0.0.16",
    "@radix-ui/react-radio-group": "^0.1.5",
    "@radix-ui/react-slider": "^0.0.16",
    "@swc/helpers": "^0.3.8",
    "react-dropzone": "^12.0.4",
    "react-select": "^5.2.2",
    "regenerator-runtime": "^0.13.9",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "downshift": "^4.x",
    "framer-motion": "^4.x",
    "react": "^17.x",
    "react-dom": "^17.x",
    "react-hotkeys-hook": "^3.x",
    "react-is": "^17.x",
    "react-tooltip": "^4.x",
    "styled-components": "^5.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build:custom": "node build.mjs",
    "build": "yarn lint:prod && yarn build:parcel",
    "prebuild:parcel": "rm -rf ./dist",
    "build:parcel": "parcel build --detailed-report --no-source-maps",
    "ppostbuild:parcel": "cp -r ./src/styles/*.css ./dist && node dist/setupPackage.js",
    "build:icons": "parcel build 'src/icons/*.tsx'",
    "build:index": "parcel build 'src/index.ts' --detailed-report",
    "build:components": "parcel build 'src/components/*/index.ts'",
    "build-storybook": "build-storybook",
    "dev": "concurrently 'yarn start' 'yarn proxy' 'yarn open'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:prod": "eslint src --ext .ts,.tsx -c ./.eslintrc.prod.js",
    "open": "open -a 'Google Chrome' https://amino-dev.zonos.com:6007",
    "proxy": "local-ssl-proxy --source 6007 --target 6006",
    "svgs:optimize": "svgo -f ./svgReact/smaller-flags -o ./svgReact/svgs",
    "svgs:download:flags": "node ./svgReact/downloadFlags.js",
    "svgs:copy:flags": "node ./svgReact/copySmallerFlags.js",
    "svgs:create": "yarn svgs:react && yarn svgs:format && yarn svgs:index",
    "svgs:format": "eslint svgReact --ext .tsx --fix",
    "svgs:index": "node ./svgReact/createIndexFile.js ./svgReact/flags",
    "svgs:react": "node ./svgReact ./svgReact/svgs ./svgReact/flags",
    "start": "yarn build-storybook && yarn storybook",
    "storybook": "start-storybook -p 6006"
  }
}
