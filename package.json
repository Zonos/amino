{
  "name": "@zonos/amino",
  "version": "5.8.0",
  "description": "Core UI components for Amino",
  "repository": "git@github.com:Zonos/amino.git",
  "license": "MIT",
  "private": false,
  "homepage": "https://amino.zonos.com",
  "engines": {
    "node": "^20 || ^22"
  },
  "packageManager": "pnpm@8.15.8",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build:ci": "pnpm build:theme && pnpm build:scss:types && pnpm build:ts && pnpm build:dts",
    "build:dts": "tspc -p ./tsconfig.emitDeclarationOnly.json",
    "extract:mcp": "tsx build-utils/mcp/build-integration.ts",
    "extract:mcp:verbose": "tsx build-utils/mcp/build-integration.ts --verbose",
    "build:prep:publish": "tsx build-utils/setupPackage.ts",
    "build:scss:types": "pnpm typed-scss-modules src/ --implementation sass --includePaths src/styles",
    "build:theme": "tsx build-utils/css/buildTheme.ts",
    "build:ts": "tsx build-utils/build.ts",
    "build": "pnpm install && pnpm ci:concurrent && pnpm build:theme && pnpm build:ts && pnpm build:dts",
    "build:mcp-server": "next build",
    "ci:concurrent": "concurrently 'pnpm tsc' 'pnpm lint:prod' 'pnpm test:prod' --group --raw --success='all'",
    "ci": "pnpm tsc && pnpm lint:prod && pnpm test:prod",
    "dev:open": "concurrently 'pnpm dev' 'pnpm open'",
    "dev": "pnpm build:scss:types --logLevel silent && concurrently 'pnpm build:scss:types --watch --ignoreInitial' 'caddy run --config  $HOME/caddy/Caddyfile' 'pnpm storybook:dev'",
    "dev:mcp": "next dev -p 3000",
    "test:app-router": "node test-app-router.js",
    "lint:fix": "pnpm lint --fix",
    "lint:prod": "pnpm lint -c ./eslint.config.prod.mjs",
    "lint": "eslint",
    "lint:copy": "eslint . -c ./eslint.config.mjs --fix 2>&1 | tee /dev/tty | pbcopy",
    "open": "open https://dev.amino.zonos.com",
    "postbuild:ci": "pnpm postbuild",
    "postbuild:dts": "echo 'done build dts'",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "postbuild:theme": "echo 'done generating styles'",
    "postbuild": "cp -r ./src/styles/*.css ./dist && cp -r ./src/styles/*.scss ./dist && pnpm build:prep:publish",
    "prebuild:ci": "pnpm prebuild",
    "prebuild:dts": "echo 'building dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "prebuild:theme": "echo 'start generating styles'",
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "push:tags": "git push origin $(git rev-parse --abbrev-ref HEAD) --follow-tags",
    "serve": "pnpm dlx http-server ./storybook-static",
    "start": "pnpm storybook build && pnpm storybook:dev",
    "storybook:build": "storybook build",
    "storybook:dev": "cross-env __VITE_ADDITIONAL_SERVER_ALLOWED_HOSTS=dev.amino.zonos.com storybook dev -p 6006 --no-open",
    "svgs:react:flags": "tsx ./svgReact/flags",
    "svgs:react:icons": "tsx ./svgReact/icons",
    "template:logic-constant": "tsx build-utils/css/generateLogicConstTemplate.ts",
    "test:prod": "vitest run",
    "test:pw": "playwright test",
    "test:sb": "test-storybook",
    "test:all": "pnpm test:prod && pnpm test:pw && pnpm test:sb",
    "test": "vitest --run",
    "test:copy": "vitest run 2>&1 | tee /dev/tty | pbcopy",
    "translate": "cd ../zonos.com && pnpm gen:translate:inline:amino",
    "tsc": "tsc --noEmit",
    "tsc:copy": "tsc --noEmit 2>&1 | tee /dev/tty | pbcopy",
    "version:major": "pnpm build && pnpm version major -m 'Bump version %s' && pnpm push:tags",
    "version:minor": "pnpm build && pnpm version minor -m 'Bump version %s' && pnpm push:tags",
    "version:patch": "pnpm build && pnpm version patch -m 'Bump version %s' && pnpm push:tags"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.0",
    "@mui/material": "^5.14.0",
    "@rollup/plugin-json": "^6.1.0",
    "@t3-oss/env-core": "^0.13.4",
    "clsx": "^2.0.0",
    "d3-geo": "^2.0.2",
    "fuse.js": "^6.6.2",
    "next": "^15.5.10",
    "react-data-grid": "7.0.0-beta.34",
    "react-dropzone": "^12.1.0",
    "react-intersection-observer": "^9.13.1",
    "react-select": "^5.7.4",
    "react-simple-maps": "^3.0.0",
    "react-split-it": "^2.0.0",
    "topojson-client": "^3.1.0",
    "uuid": "^9.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^5.0.0",
