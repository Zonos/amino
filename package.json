{
  "name": "@zonos/amino",
  "version": "5.5.27",
  "description": "Core UI components for Amino",
  "repository": "git@github.com:Zonos/amino.git",
  "license": "MIT",
  "private": false,
  "homepage": "https://amino.zonos.com",
  "engines": {
    "node": "^20 || ^22"
  },
  "packageManager": "pnpm@8.15.8",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build:ci": "pnpm build:theme && pnpm build:scss:types && pnpm build:ts && pnpm build:dts",
    "build:dts": "tspc -p ./tsconfig.emitDeclarationOnly.json",
    "extract:mcp": "tsx build-utils/mcp/build-integration.ts",
    "extract:mcp:verbose": "tsx build-utils/mcp/build-integration.ts --verbose",
    "build:prep:publish": "tsx build-utils/setupPackage.ts",
    "build:scss:types": "pnpm typed-scss-modules src/ --implementation sass --includePaths src/styles",
    "build:theme": "tsx build-utils/css/buildTheme.ts",
    "build:ts": "tsx build-utils/build.ts",
    "build": "pnpm install && pnpm ci:concurrent && pnpm build:theme && pnpm build:ts && pnpm build:dts",
    "ci:concurrent": "concurrently 'pnpm tsc' 'pnpm lint:prod' 'pnpm test:prod' --group --raw --success='all'",
    "ci": "pnpm tsc && pnpm lint:prod && pnpm test:prod",
    "dev:open": "concurrently 'pnpm dev' 'pnpm open'",
    "dev": "pnpm build:scss:types --logLevel silent && concurrently 'pnpm build:scss:types --watch --ignoreInitial' 'caddy run --config  $HOME/caddy/Caddyfile' 'pnpm storybook:dev'",
    "dev:mcp": "next dev -p 3000",
    "lint:fix": "pnpm lint --fix",
    "lint:prod": "pnpm lint -c ./eslint.config.prod.mjs",
    "lint": "eslint",
    "lint:copy": "eslint . -c ./eslint.config.mjs --fix 2>&1 | tee /dev/tty | pbcopy",
    "open": "open https://dev.amino.zonos.com",
    "postbuild:ci": "pnpm postbuild",
    "postbuild:dts": "echo 'done build dts'",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "postbuild:theme": "echo 'done generating styles'",
    "postbuild": "cp -r ./src/styles/*.css ./dist && cp -r ./src/styles/*.scss ./dist && pnpm build:prep:publish",
    "prebuild:ci": "pnpm prebuild",
    "prebuild:dts": "echo 'building dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "prebuild:theme": "echo 'start generating styles'",
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "push:tags": "git push origin $(git rev-parse --abbrev-ref HEAD) --follow-tags",
    "serve": "pnpm dlx http-server ./storybook-static",
    "start": "pnpm storybook build && pnpm storybook:dev",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "svgs:react:flags": "tsx ./svgReact/flags",
    "svgs:react:icons": "tsx ./svgReact/icons",
    "template:logic-constant": "tsx build-utils/css/generateLogicConstTemplate.ts",
    "test:prod": "vitest run",
    "test:pw": "playwright test",
    "test:sb": "test-storybook",
    "test:all": "pnpm test:prod && pnpm test:pw && pnpm test:sb",
    "test": "vitest --run",
    "test:copy": "vitest run 2>&1 | tee /dev/tty | pbcopy",
    "tsc": "tsc --noEmit",
    "tsc:copy": "tsc --noEmit 2>&1 | tee /dev/tty | pbcopy",
    "version:major": "pnpm build && pnpm version major -m 'Bump version %s' && pnpm push:tags",
    "version:minor": "pnpm build && pnpm version minor -m 'Bump version %s' && pnpm push:tags",
    "version:patch": "pnpm build && pnpm version patch -m 'Bump version %s' && pnpm push:tags"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.0",
    "@mui/material": "^5.14.0",
    "clsx": "^2.0.0",
    "d3-geo": "^2.0.2",
    "fuse.js": "^6.6.2",
    "next": "^15.3.1",
    "react-data-grid": "7.0.0-beta.34",
    "react-dropzone": "^12.1.0",
    "react-intersection-observer": "^9.13.1",
    "react-select": "^5.7.4",
    "react-simple-maps": "^3.0.0",
    "react-split-it": "^2.0.0",
    "topojson-client": "^3.1.0",
    "uuid": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@eslint/js": "^9.16.0",
    "@playwright/test": "1.51.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-a11y": "^8.6.4",
    "@storybook/addon-designs": "^8.2.1",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.4",
    "@storybook/manager-api": "^8.6.4",
    "@storybook/preview-api": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/theming": "^8.6.4",
    "@storybook/types": "^8.6.11",
    "@testing-library/react": "^15.0.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/d3-geo": "^3.0.3",
    "@types/geojson": "^7946.0.10",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/next": "^9.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/react-simple-maps": "^3.0.0",
    "@types/topojson-client": "^3.1.1",
    "@types/topojson-specification": "^1.0.2",
    "@types/uuid": "^9.0.2",
    "@vitest/eslint-plugin": "^1.1.15",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys": "2.3.5",
    "eslint-plugin-storybook": "^0.11.4",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "framer-motion": "^10.16.4",
    "glob": "^10.3.3",
    "globals": "^15.13.0",
    "graphql": "^16.7.1",
    "graphql-request": "~5.1.0",
    "graphql-ws": "4.5.0",
    "jsdom": "^23.2.0",
    "lodash": "^4.17.21",
    "postcss": "8.4.31",
    "postcss-preset-env": "9.0.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^4.5.1",
    "rollup": "^3.26.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.4",
    "storybook-addon-tag-badges": "^1.4.0",
    "style-loader": "^3.3.3",
    "svgo": "^3.0.2",
    "swr": "^2.2.5",
    "ts-patch": "3.0.1",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typed-scss-modules": "8.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.18.0",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^6.2.2",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.x",
    "@emotion/styled": "^11.x",
    "dayjs": "*",
    "framer-motion": "^10.x",
    "graphql": "^16.x",
    "graphql-request": "~5.1.0",
    "lodash": "^4.17.21",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x",
    "react-tooltip": "^4.x",
    "swr": "2.x"
  }
}
