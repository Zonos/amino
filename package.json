    "@eslint/js": "^9.16.0",
    "@playwright/test": "1.58.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-a11y": "^10.2.4",
    "@storybook/addon-designs": "^11.1.1",
    "@storybook/addon-docs": "^10.2.4",
    "@storybook/addon-links": "^10.2.4",
    "@storybook/addon-onboarding": "^10.2.4",
    "@storybook/addon-vitest": "^10.2.4",
    "@storybook/react": "^10.2.4",
    "@storybook/react-vite": "^10.2.4",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/d3-geo": "^3.0.3",
    "@types/geojson": "^7946.0.10",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^25.2.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/react-simple-maps": "^3.0.0",
    "@types/topojson-client": "^3.1.1",
    "@types/topojson-specification": "^1.0.2",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.6",
    "autoprefixer": "10.4.24",
    "concurrently": "^8.2.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys": "2.3.5",
    "eslint-plugin-storybook": "^10.2.4",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "framer-motion": "^10.16.4",
    "glob": "^10.5.0",
    "globals": "^15.13.0",
    "graphql": "^16.7.1",
    "graphql-request": "^7.4.0",
    "graphql-ws": "4.5.0",
    "jsdom": "^28.0.0",
    "lodash": "^4.17.21",
    "playwright": "^1.58.1",
    "postcss": "8.5.6",
    "postcss-preset-env": "11.1.2",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^4.5.1",
    "rollup": "^3.26.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.5",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "storybook": "^10.2.4",
    "style-loader": "^4.0.0",
    "svgo": "^3.0.2",
    "swr": "^2.2.5",
    "ts-patch": "3.3.0",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typed-scss-modules": "8.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.18.0",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "d3-color": "^3.1.0",
      "qs": "^6.14.1",
      "axios": "^1.12.0",
      "vite": "^6.4.1"
    }
  },
  "peerDependencies": {
    "@emotion/react": "^11.x",
    "@emotion/styled": "^11.x",
    "dayjs": "*",
    "framer-motion": "^10.x",
    "graphql": "^16.x",
