  "scripts": {
    "build:ci": "pnpm build:theme && ts-node build-utils/build.ts && pnpm build:dts",
    "build:colors": "ts-node build-utils/css/buildColorConstants.ts",
    "build:dts": "tspc -p ./tsconfig.emitDeclarationOnly.json",
    "build:logic-constant": "ts-node build-utils/css/buildLogicConstants.ts",
    "build:prep:publish": "ts-node build-utils/setupPackage.ts",
    "build:theme": "ts-node build-utils/css/buildTheme.ts",
    "build": "pnpm install && pnpm ci:concurrent && pnpm build:theme && ts-node build-utils/build.ts && pnpm build:dts",
    "ci:concurrent": "concurrently 'pnpm tsc' 'pnpm lint:prod' 'pnpm test:prod' --group --raw --success='all'",
    "ci": "pnpm tsc && pnpm lint:prod && pnpm test:prod",
    "dev:open": "concurrently 'pnpm storybook' 'pnpm proxy' 'pnpm open'",
    "dev": "concurrently 'pnpm storybook' 'pnpm proxy'",
    "lint:prod": "eslint src --ext .ts,.tsx -c ./.eslintrc.prod.js",
    "lint": "eslint src --ext .ts,.tsx",
    "open": "open https://dev.amino.zonos.com:6007",
    "postbuild:ci": "pnpm postbuild",
    "postbuild:dts": "echo 'done build dts'",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "postbuild:theme": "echo 'done generating styles'",
    "postbuild": "cp -r ./src/styles/*.css ./dist && pnpm build:prep:publish",
    "prebuild:ci": "pnpm prebuild",
    "prebuild:dts": "echo 'building dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "prebuild:theme": "echo 'start generating styles'",
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "presvgs:react:flags": "ts-node ./svgReact/flags/runGenerateSvgs.ts",
    "proxy": "pnpm proxy:options --source 6007 --target 6006",
    "proxy:options": "NODE_OPTIONS=--dns-result-order=ipv4first local-ssl-proxy",
    "push:tags": "git push --follow-tags",
    "start": "pnpm storybook build && pnpm storybook",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "svgs:format": "eslint --fix svgReact --ext .tsx -c ./.eslintrc.prod.js",
    "svgs:react:flags": "ts-node ./svgReact/flags/runCreateComponentsFromSvgs.ts",
    "svgs:react:icons": "ts-node ./svgReact/icons",
    "template:logic-constant": "ts-node build-utils/css/generateLogicConstTemplate.ts",
    "test:prod": "jest --testPathIgnorePatterns storyshots",
    "test:visual": "jest ./.storybook/__tests__/storyshots",
    "test:pw": "playwright test",
    "test": "jest --watchAll --testPathIgnorePatterns storyshots",
    "tsc": "tsc --noEmit",
    "version:major": "pnpm build && npm version major -m 'Bump version %s' && pnpm push:tags",
    "version:minor": "pnpm build && npm version minor -m 'Bump version %s' && pnpm push:tags",
    "version:patch": "pnpm build && npm version patch -m 'Bump version %s' && pnpm push:tags"
  }
