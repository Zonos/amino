  },
  "peerDependencies": {
    "downshift": "^4.x",
    "framer-motion": "^4.x",
    "react": "^17.x",
    "react-dom": "^17.x",
    "react-hotkeys-hook": "^3.x",
    "react-is": "^17.x",
    "react-tooltip": "^4.x",
    "styled-components": "^5.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "yarn lint:prod && bili && cp -r ./src/styles/*.css ./dist",
    "build-storybook": "build-storybook",
    "build:watch": "bili --watch",
    "dev": "concurrently 'yarn start' 'yarn proxy' 'yarn open'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:prod": "eslint src --ext .ts,.tsx -c ./.eslintrc.prod.js",
    "open": "open -a 'Google Chrome' https://amino-dev.zonos.com:6007",
    "proxy": "local-ssl-proxy --source 6007 --target 6006",
    "svgs:create": "yarn svgs:react && yarn svgs:format && yarn svgs:index",
    "svgs:format": "eslint svgReact --ext .tsx --fix",
    "svgs:index": "node ./svgReact/createIndexFile.js ./svgReact/reactSVGs",
    "svgs:react": "node ./svgReact ./svgReact/svgs ./svgReact/reactSVGs",
    "start": "yarn build-storybook && yarn storybook",
    "storybook": "start-storybook -p 6006"
  }
